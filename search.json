---
layout: none
---
[
    {% for post in site.posts %}{
        "title": "{{ post.title | escape }}",
        "tags": "{{ post.tags | join: ', ' }}",
        "url": "{{ site.url }}{{ post.url }}",
        "description": "{{ post.description }}",
        "date": "{{ post.date | date: '%d %b %Y' }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]